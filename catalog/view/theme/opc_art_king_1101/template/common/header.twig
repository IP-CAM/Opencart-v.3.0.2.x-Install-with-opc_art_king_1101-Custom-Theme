<body id="{% if homeid %}{{ homeid }}{% else %}noid{% endif %}" class="lang-es {% if direction != 'ltr' %} lang-rtl {% endif %}  country-us currency-usd layout-full-width page-index tax-display-disabled"  data-menu-sticky='{{ mainmenustickystatus }}' style="{{ tvcmscustomsetting_background_style_sheet }}" >

	{{ themeoption }}
	{% if bottomoption %}<div class="bottomoption hidden">{{ bottomoption }}</div>{% endif %}
	{% if mousehoverimage %}<div class="mousehoverimage hidden">{{ mousehoverimage }}</div>{% endif %}
	{% if mainmenustickystatus %}<div class="mainmenustickystatus hidden">{{ mainmenustickystatus }}</div>{% endif %}
			{% if pageloader %}
			<div class="ttvcms-loading-overlay">
				<div class="ttvcms-loading-inner">
					<img class="logo img-responsive" src="{{ logo }}" alt="{{ name }}">
					<img src="{{ loader_img }}" alt="{{ loader_img }}">
					</div>
				</div>
			{% endif %}
			<div class="ttv-main-div{% if boxlayout %}ttv-box-layout container{% endif %}">
				<header id="header" class="">
					<div class="ttvcmsheader-banner"></div>
					<div class="ttvcmsdesktop-top-header-wrapper">
						<div class="container">
							<div class="row">
								<div class="col-md-4 hidden-sm-down ttvcms-header-logo">
									<div id="ttvdesktop-header-myaccount-wrapper">
										<div class="ttvcms-header-myaccount">
											<div class="ttv-header-account">
												<div class="ttv-account-wrapper"><button class="btn-unstyle ttv-myaccount-btn"> <i class="fa fa-user" aria-hidden="true"></i>
													</button>
													<div class="dropdown-menu ttv-account-dropdown ttv-dropdown" style="display: none;">
														<!-- begin module:opc_art_king_1101\template\common\language.twig -->
														<!-- begin D:\xampp\htdocs\opencart\hrx\hrx_101\catalog\view\theme\opc_art_king_1101\template\common\language.twig -->
														{{ language }}
														<!-- end D:\xampp\htdocs\opencart\hrx\hrx_101\catalog\view\theme\opc_art_king_1101\template\common\language.twig -->
														<!-- end module:opc_art_king_1101\template\common\language.twig -->
														<!-- begin module:opc_art_king_1101\template\common\currency.twig -->
														<!-- begin  D:\xampp\htdocs\opencart\hrx\hrx_101\catalog\view\theme\opc_art_king_1101\template\common\currency.twig -->
														{{ currency }}
														<!-- end  D:\xampp\htdocs\opencart\hrx\hrx_101\catalog\view\theme\opc_art_king_1101\template\common\currency.twig -->
														<!-- end module:opc_art_king_1101\template\common\currency.twig -->
														<div id="ttvcmsdesktop-user-info" class="ttvcms-header-sign">
															<div class="user-info ttvheader-sign">
																{% if logged %}
																<a class="logout ttvhedaer-sign-btn" href="{{ logout }}"> {{ text_logout }}</a>
																<a class="account ttvhedaer-sign-btn"  href="{{ account }}" title="View my customer account"><span class="ttvhedaer-sign-span">{{ text_account }}</span></a>
																{% else %}
																<a href="{{ login }}" class="ttvhedaer-sign-btn" title="Log in to your customer account">
																<span class="ttvhedaer-sign-span">{{ text_login }}</span>
															</a>
																{% endif %}
															</div>
														</div>														
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
								<div class="col-md-4 hidden-sm-down ttvcms-header-logo" id="ttvcmsdesktop-logo">
									<div class="ttv-header-logo">
										{% if logo %}
										<a href="{{ home }}" class="ttvheader-top-logo-wrapper">
										<img class="logo img-responsive" src="{{ logo }}" title="{{ name }}" alt="{{ name }}">
									</a> {% else %}
										<h1><a href="{{ home }}" class="ttvheader-top-logo-wrapper">{{ name }}</a></h1> {% endif %}
									</div>
								</div>								
								<div class="col-md-4 col-sm-12 ttvcmsheader-nav-right">
									<div class="ttv-search-account-cart-wrapper">
										<div class="ttvcmssearch-wrapper" id="_desktop_search">
											<!-- begin catalog\view\theme\opc_art_king_1101\template\common\search.twig -->
											{{ search }}
											<!-- end catalog\view\theme\opc_art_king_1101\template\common\search.twig -->
										</div>
										<!-- begin module:opc_art_king_1101\template\common\cart.twig -->
										<!-- begin D:\xampp\htdocs\opencart\hrx\hrx_101\catalog\view\theme\opc_art_king_1101\template\common\cart.twig -->
										{{ cart }}
										<!-- end D:\xampp\htdocs\opencart\hrx\hrx_101\catalog\view\theme\opc_art_king_1101\template\common\cart.twig -->
										<!-- end module:opc_art_king_1101\template\common\cart.twig -->
									</div>
								</div>
								<div class="col-md-12 col-sm-12 position-static ttvcms-header-menu">									
									<!-- begin module:opc_art_king_1101\template\common\menu.twig -->
									<!-- begin D:\xampp\htdocs\opencart\hrx\hrx_101\catalog\view\theme\opc_art_king_1101\template\common\menu.twig -->
									<div class="ttvcmsmain-menu-outer">
										{{ menu }}
									</div>
									<!-- end D:\xampp\htdocs\opencart\hrx\hrx_101\catalog\view\theme\opc_art_king_1101\template\common\menu.twig -->
									<!-- end module:opc_art_king_1101\template\common\menu.twig -->
								</div>
							</div>
						</div>
					</div>
